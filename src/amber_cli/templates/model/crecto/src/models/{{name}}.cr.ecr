class <%= class_name %> < Crecto::Model
  schema "<%= table_name %>" do
<%- @fields.reject{|f| f.hidden }.each do |field| -%>
  <%- if field.reference? -%>
    belongs_to :<%= field.name %>, <%= field.name.camelcase %>
  <%- else -%>
    field :<%= field.name %>, <%= field.cr_type %>
  <%- end -%>
<%- end -%>
  end
end
